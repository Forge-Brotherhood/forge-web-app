{
  "name": "forge-web-app",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=22.12.0 <23"
  },
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "vercel-build": "if [ \"$VERCEL_ENV\" = \"production\" ] && [ \"$RUN_PRISMA_MIGRATIONS\" = \"true\" ]; then prisma migrate deploy; fi && prisma generate && next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "test": "node --test --import tsx src/lib/pipeline/plan/planBuilder.test.ts",
    "db:up": "docker compose --env-file env.example up -d",
    "db:down": "docker compose down",
    "db:extensions": "prisma db execute --file prisma/enable-citext.sql",
    "db:push": "prisma db push",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:baseline": "tsx scripts/baseline-prisma-migrations.ts",
    "db:bootstrap": "npm run db:extensions && npm run db:push && npm run db:migrate:baseline",
    "db:backfill:bible-ranges": "prisma db execute --file prisma/manual/backfill-bible-ranges.sql",
    "db:backfill:group-type:add-values": "prisma db execute --file prisma/manual/group-type-add-values.sql",
    "db:backfill:group-type:remap": "prisma db execute --file prisma/manual/group-type-remap-values.sql",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@clerk/nextjs": "^6.31.6",
    "@eslint/eslintrc": "^3.3.1",
    "@hookform/resolvers": "^5.2.1",
    "@mux/mux-node": "^8.0.0",
    "@mux/mux-player": "^3.5.3",
    "@mux/mux-player-react": "^3.5.3",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.87.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.8",
    "@vercel/kv": "^3.0.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.9.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.2.3",
    "firebase-admin": "^13.6.0",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.542.0",
    "nanoid": "^5.1.5",
    "next": "^15.2.3",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "photoswipe": "^5.4.4",
    "postcss": "^8.5.6",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "^7.62.0",
    "react-photoswipe-gallery": "^3.1.1",
    "svix": "^1.75.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@next/bundle-analyzer": "^15.2.3",
    "@types/pg": "^8.11.10",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0"
  },
  "overrides": {
    "formdata-node": "6.0.3"
  }
}
